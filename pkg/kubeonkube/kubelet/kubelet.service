[Unit]
Description=Kubernetes Kubelet
After=docker.service
[Service]
ExecStart=/usr/bin/kubelet --kubeconfig=/etc/kubernetes/node.config --config=/etc/kubernetes/kubelet-config.yaml --network-plugin=cni --pod-infra-container-image=registry.aliyuncs.com/google_containers/pause:3.1
Restart=on-failure
LimitNOFILE=65536
[Install]
WantedBy=multi-user.target